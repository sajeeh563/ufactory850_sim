<?xml version="1.0"?>
<robot name="ufactory850" xmlns:xacro="http://ros.org/wiki/xacro">
  <link name="base_link">
    <visual>
      <geometry><cylinder radius="0.05" length="0.02"/></geometry>
      <material name="gray"><color rgba="0.5 0.5 0.5 1"/></material>
    </visual>
  </link>
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.02" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="5" velocity="2"/>
  </joint>
  <link name="link1">
    <visual>
      <geometry><box size="0.05 0.05 0.2"/></geometry>
      <material name="silver"><color rgba="0.7 0.7 0.7 1"/></material>
    </visual>
  </link>
  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2"/>
  </joint>
  <link name="link2">
    <visual>
      <geometry><box size="0.05 0.05 0.15"/></geometry>
      <material name="silver"/>
    </visual>
  </link>
  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="gripper"/>
    <origin xyz="0 0 0.15" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="5" velocity="2"/>
  </joint>
  <link name="gripper">
    <visual>
      <geometry><box size="0.02 0.02 0.05"/></geometry>
      <material name="blue"><color rgba="0 0 1 1"/></material>
    </visual>
  </link>
</robot>
